        <!-- Nav tabs -->
        <div class='col-md-6' style='margin-left: 25%;'>
          <ul class='nav nav-tabs nav-justified'>
            <li class='active'>
              <a data-toggle='tab' href='#today'>Today</a>
            </li>
            <li>
              <a data-toggle='tab' href='#week'>Week</a>
            </li>
            <li>
