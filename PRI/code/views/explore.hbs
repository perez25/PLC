    <div class='container tab-content'>
      <!-- Today -->
      <div class='row tab-pane fade fade in active' id='today'>
        <div class='col-md-6'>
          <article class='explore-module'>
            <h1 class='h2 module-title'>
              <span class='icon icon-like-heart-fill'></span>
              What's popular?
            </h1>
            <div class='module-box'>
              {{#if popular}}
                {{#each popular}}
                  <div class='row box-row'>
                    <div class='col-md-4'>
                      <div class='image-wrap'>
                        <img class='guidePicture img-responsive' data-src='{{ places.0.picture }}'>
                      </div>
                    </div>
                    <div class='col-md-8 wrap'>
                      <header>
                        <h1 class='h5'>
                          <a href='/guide/{{ _id }}'>{{{trimString title 25}}}</a>
                        </h1>
                        <p>{{{trimString description 130}}}</p>
                      </header>
                      <div class='user-rating small'>
                         <div class='numbers'>
                          {{#if avg }} {{ avg }} {{else}} 0.0 {{/if}}
                        </div>
                      </div>
                      <div class='user-name'>
                        <img alt='user picture' class='userPicture img-circle' src='{{ author.picture }}'>
                        by
                        <a href='/profile/{{ author._id }}'>{{ author.name.full }}</a>
                      </div>
                      <div class='icons pull-right'>
                        <span>
                          <a class='icon icon-favorite-star-fill' href='#'>
                            <span>{{ favorites.length }}</span>
                          </a>
                        </span>
                        <span>
                          <a class='icon icon-bubble-chat-fill' href='#'>
                            <span>{{ reviews.length }}</span>
                          </a>
                        </span>
                      </div>
                    </div>
                  </div>
                {{/each}}
              {{/if}}
            </div>
          </article>
        </div>
